    "@malloydata/malloy-explorer": "^0.0.305-dev250819202105",
    "@malloydata/malloy-interfaces": "^0.0.330",
    "@malloydata/malloy-query-builder": "^0.0.330",
    "@malloydata/render": "^0.0.330",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-router": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "@malloydata/db-duckdb": {
      "@duckdb/duckdb-wasm": "$@duckdb/duckdb-wasm"
    }
  }
