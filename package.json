{
  "name": "@aszenz/data-explorer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "prettier --check . && tsc --noEmit && eslint .",
